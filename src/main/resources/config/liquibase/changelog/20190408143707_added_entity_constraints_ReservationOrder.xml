<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity ReservationOrder.
    -->
    <changeSet id="20190408143707-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="event_date_id"
                                 baseTableName="reservation_order"
                                 constraintName="fk_reservation_order_event_date_id"
                                 referencedColumnNames="id"
                                 referencedTableName="offer"/>

        <addForeignKeyConstraint baseColumnNames="required_id"
                                 baseTableName="reservation_order"
                                 constraintName="fk_reservation_order_required_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

    </changeSet>
</databaseChangeLog>
